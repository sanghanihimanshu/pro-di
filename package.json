{
  "name": "pro-di",
  "type": "module",
  "version": "1.0.3",
  "description": "A professional DI container for Node.js/TypeScript inspired by Koin with global and request-based injection support for Hono and Express.",
  "author": "himanshu patel",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanghanihimanshu/pro-di.git"
  },
  "bugs": {
    "url": "https://github.com/sanghanihimanshu/pro-di/issues"
  },
  "keywords": [
    "dependency-injection",
    "di",
    "typescript",
    "container",
    "global",
    "request-scope",
    "express",
    "hono",
    "decorators"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "tsc --build --force && tsc-alias",
    "prepublishOnly": "npm run build",
    "test": "vitest --allowOnly",
    "test:coverage": "vitest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-node": "^0.57.2",
    "express": "^4.21.2",
    "hono": "^4.6.20",
    "reflect-metadata": "^0.1.14"
  },
  "devDependencies": {
    "@antfu/eslint-config": "3.16.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.1",
    "@vitest/coverage-v8": "^3.0.5",
    "eslint": "^9.19.0",
    "eslint-plugin-format": "^0.1.3",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.0.0",
    "vitest": "^3.0.4"
  }
}
